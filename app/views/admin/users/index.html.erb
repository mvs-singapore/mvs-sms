<h1>Manage Users</h1>

<p><%= link_to('Add User', new_admin_user_path) %></p>

<ul>
<% @users.each do |user| %>
  <li>
    <%= link_to user.name, edit_admin_user_path(user), class: "edit_user" %>
    [ <%= link_to 'delete', admin_user_path(user), method: :delete, data: { confirm: 'Are you sure?' }, class: "delete_user" %> ]
  </li>
<% end %>
</ul>

