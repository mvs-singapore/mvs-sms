<div class="form-group row">
  <div class="col-6">
    <%= f.label :school_name, 'School Attended' %>
    <%= f.text_field :school_name, autofocus: true, class: 'form-control' %>
  </div>

  <div class="col-6">
    <%= f.label :from_date, 'From Date' %>
    <%= f.date_field :from_date, autofocus: true, class: 'form-control' %>
  </div>

  <div class="col-6">
    <%= f.label :to_date, 'To Date' %>
    <%= f.date_field :to_date, autofocus: true, class: 'form-control' %>
  </div>

  <div class="col-6">
    <%= f.label :qualification %>
    <%= f.text_field :qualification, autofocus: true, class: 'form-control' %>
  </div>
</div>

<div class="form-group row">
  <div class="col-4"><%= f.submit :class => 'btn btn-primary' %></div>
</div>
